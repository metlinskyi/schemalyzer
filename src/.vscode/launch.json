{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Console",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "Console.dll",
            "args": ["MsSql.dll"],
            "cwd": "${workspaceFolder}/../bin/Debug/app/",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "xUnit",
            "type": "coreclr",
            "request": "launch",
            "program": "dotnet",
            "args": ["test", "tests/tests.csproj", "--nologo"],
            "cwd": "${workspaceFolder}/../",
            "stopAtEntry": false
        },
        {
            "name": "Build MsSql",
            "type": "coreclr",
            "request": "launch",
            "program": "dotnet",
            "args": ["build", "${workspaceFolder}/Providers/MsSql/MsSql.csproj"],
            "cwd": "${workspaceFolder}/../bin",
            "stopAtEntry": false
        }
    ]
}