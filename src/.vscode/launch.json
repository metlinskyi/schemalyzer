{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Console",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/../bin/Debug/netcoreapp3.1/app/Console.dll",
            "args": [],
            "cwd": "${workspaceFolder}/../bin",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "xUnit",
            "type": "coreclr",
            "request": "launch",
            "program": "dotnet",
            "args": ["test", "${workspaceFolder}/../tests/tests.csproj"],
            "cwd": "${workspaceFolder}/../bin",
            "stopAtEntry": false
        },
        {
            "name": "Build MsSql",
            "type": "coreclr",
            "request": "launch",
            "program": "dotnet",
            "args": ["build", "${workspaceFolder}/Providers/MsSql/MsSql.csproj"],
            "cwd": "${workspaceFolder}/..",
            "stopAtEntry": false
        }
    ]
}